function malfunction()
  if got_equib then
    send("dohyponsisthingyhere")
    return true
  elseif got_balance then
    send("dodstabthingyhere")
    return 1.4 --set a custom timeout to 1.4s - if in 1.4s we didn't punch, the function will be run again for us
  end

  return false
end

svo.addbalanceful("maliki function", malfunction)
